{
    "name": "swoole/test",
    "type": "project",
    "keywords": [
        "php",
        "swoole",
        "framework",
        "hyperf",
        "microservice",
        "middleware"
    ],
    "description": "A swoole test project.",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.4",
        "ext-swoole": "^6.0",
        "nikic/fast-route": "^1.3",
        "ramsey/uuid": "^4.9"
    },
    "require-dev": {
        "swoole/ide-helper": "*"
    },
    "suggest": {
        "ext-openssl": "Required to use HTTPS.",
        "ext-json": "Required to use JSON.",
        "ext-pdo": "Required to use MySQL Client.",
        "ext-pdo_mysql": "Required to use MySQL Client.",
        "ext-redis": "Required to use Redis Client."
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": [],
    "scripts": {},
    "repositories": []
}
